<!-- Perfect Modern Events Dashboard -->
<div class="min-h-screen bg-gradient-to-br from-slate-50 via-blue-50 to-indigo-50">
  <!-- Sticky Header with Navigation -->
  <header class="sticky top-0 z-50 bg-white/80 backdrop-blur-md border-b border-gray-200/60 shadow-sm">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="flex justify-between items-center h-16">
        <div class="flex items-center space-x-4">
          <div class="flex-shrink-0">
            <div class="h-10 w-10 bg-gradient-to-r from-blue-600 to-indigo-600 rounded-xl flex items-center justify-center shadow-lg">
              <svg class="h-6 w-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v10a2 2 0 002 2h8a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2"></path>
              </svg>
            </div>
          </div>
          <div>
            <h1 class="text-xl font-bold text-gray-900">Event Analytics</h1>
            <p class="text-sm text-gray-600">Monitor and analyze your application events in real-time</p>
          </div>
        </div>
        <div class="flex items-center space-x-3">
          <%= link_to dashboard_path, class: "inline-flex items-center px-4 py-2 border border-gray-300 shadow-sm text-sm font-medium rounded-lg text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500 transition-all duration-200" do %>
            <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6"></path>
            </svg>
            Dashboard
          <% end %>
          <%= link_to events_path(format: :csv), class: "inline-flex items-center px-4 py-2 border border-transparent text-sm font-medium rounded-lg shadow-sm text-white bg-gradient-to-r from-emerald-500 to-teal-600 hover:from-emerald-600 hover:to-teal-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-emerald-500 transition-all duration-200" do %>
            <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 10v6m0 0l-3-3m3 3l3-3m2 8H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
            </svg>
            Export CSV
          <% end %>
        </div>
      </div>
    </div>
  </header>

  <div class="max-w-7xl mx-auto py-8 px-4 sm:px-6 lg:px-8">
    <!-- Enhanced Stats Grid -->
    <div class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-4 mb-8">
      <!-- Total Events Card -->
      <div class="bg-white/80 backdrop-blur-sm overflow-hidden shadow-xl rounded-2xl border border-gray-100 hover:shadow-2xl transition-all duration-300 transform hover:-translate-y-1">
        <div class="p-6">
          <div class="flex items-center">
            <div class="flex-shrink-0">
              <div class="h-12 w-12 bg-gradient-to-r from-blue-500 to-blue-600 rounded-xl flex items-center justify-center shadow-lg">
                <svg class="h-6 w-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
                </svg>
              </div>
            </div>
            <div class="ml-5 w-0 flex-1">
              <dl>
                <dt class="text-sm font-medium text-gray-600 truncate">Total Events</dt>
                <dd class="text-3xl font-bold text-gray-900"><%= @event_stats[:total_events] || 0 %></dd>
              </dl>
            </div>
          </div>
        </div>
      </div>

      <!-- Today's Events Card -->
      <div class="bg-white/80 backdrop-blur-sm overflow-hidden shadow-xl rounded-2xl border border-gray-100 hover:shadow-2xl transition-all duration-300 transform hover:-translate-y-1">
        <div class="p-6">
          <div class="flex items-center">
            <div class="flex-shrink-0">
              <div class="h-12 w-12 bg-gradient-to-r from-amber-400 to-orange-500 rounded-xl flex items-center justify-center shadow-lg">
                <svg class="h-6 w-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                </svg>
              </div>
            </div>
            <div class="ml-5 w-0 flex-1">
              <dl>
                <dt class="text-sm font-medium text-gray-600 truncate">Today</dt>
                <dd class="text-3xl font-bold text-gray-900"><%= @event_stats[:today_events] || 0 %></dd>
              </dl>
            </div>
          </div>
        </div>
      </div>

      <!-- This Week Card -->
      <div class="bg-white/80 backdrop-blur-sm overflow-hidden shadow-xl rounded-2xl border border-gray-100 hover:shadow-2xl transition-all duration-300 transform hover:-translate-y-1">
        <div class="p-6">
          <div class="flex items-center">
            <div class="flex-shrink-0">
              <div class="h-12 w-12 bg-gradient-to-r from-emerald-400 to-teal-500 rounded-xl flex items-center justify-center shadow-lg">
                <svg class="h-6 w-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                </svg>
              </div>
            </div>
            <div class="ml-5 w-0 flex-1">
              <dl>
                <dt class="text-sm font-medium text-gray-600 truncate">This Week</dt>
                <dd class="text-3xl font-bold text-gray-900"><%= @event_stats[:this_week_events] || 0 %></dd>
              </dl>
            </div>
          </div>
        </div>
      </div>

      <!-- Story Flows Card -->
      <div class="bg-white/80 backdrop-blur-sm overflow-hidden shadow-xl rounded-2xl border border-gray-100 hover:shadow-2xl transition-all duration-300 transform hover:-translate-y-1">
        <div class="p-6">
          <div class="flex items-center">
            <div class="flex-shrink-0">
              <div class="h-12 w-12 bg-gradient-to-r from-purple-400 to-pink-500 rounded-xl flex items-center justify-center shadow-lg">
                <svg class="h-6 w-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656 0l-1.102 1.101"></path>
                </svg>
              </div>
            </div>
            <div class="ml-5 w-0 flex-1">
              <dl>
                <dt class="text-sm font-medium text-gray-600 truncate">Story Flows</dt>
                <dd class="text-3xl font-bold text-gray-900"><%= @event_stats[:story_flows_count] || 0 %></dd>
              </dl>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Advanced Filtering Section -->
    <div class="bg-white/80 backdrop-blur-sm shadow-xl rounded-2xl border border-gray-100 mb-8">
      <div class="px-6 py-6">
        <h3 class="text-lg font-semibold text-gray-900 mb-4">Filter Events</h3>
        
        <%= form_with url: events_path, method: :get, local: true, class: "space-y-6" do |form| %>
          <div class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-4">
            <!-- Search -->
            <div>
              <label class="block text-sm font-medium text-gray-700 mb-2">Search</label>
              <%= form.text_field :search, value: params[:search], placeholder: "Search events...", class: "w-full px-4 py-3 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 focus:border-blue-500 transition-all duration-200" %>
            </div>

            <!-- Event Type -->
            <div>
              <label class="block text-sm font-medium text-gray-700 mb-2">Event Type</label>
              <%= form.select :event_type, 
                  options_for_select([
                    ['All Types', ''],
                    ['HTTP Request', 'http.request'],
                    ['Data Change', 'data.change'],
                    ['Job Execution', 'job.execution']
                  ], params[:event_type]), 
                  {}, 
                  class: "w-full px-4 py-3 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 focus:border-blue-500 transition-all duration-200" %>
            </div>

            <!-- Date Range -->
            <div>
              <label class="block text-sm font-medium text-gray-700 mb-2">Date Range</label>
              <%= form.select :date_range, 
                  options_for_select([
                    ['Last 7 days', '7d'],
                    ['Last 24 hours', '24h'],
                    ['Last hour', '1h'],
                    ['Last 30 days', '30d'],
                    ['Last 90 days', '90d']
                  ], params[:date_range]), 
                  {}, 
                  class: "w-full px-4 py-3 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 focus:border-blue-500 transition-all duration-200" %>
            </div>

            <!-- Action -->
            <div>
              <label class="block text-sm font-medium text-gray-700 mb-2">Action</label>
              <%= form.text_field :action_filter, value: params[:action_filter], placeholder: "Filter by action...", class: "w-full px-4 py-3 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 focus:border-blue-500 transition-all duration-200" %>
            </div>
          </div>

          <!-- Filter Actions -->
          <div class="flex items-center justify-between pt-4 border-t border-gray-200">
            <div class="flex items-center space-x-4">
              <%= form.submit "Apply Filters", class: "inline-flex items-center px-6 py-3 border border-transparent text-sm font-medium rounded-lg text-white bg-blue-600 hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500 transition-all duration-200" %>
              <%= link_to "Clear All", events_path, class: "inline-flex items-center px-6 py-3 border border-gray-300 text-sm font-medium rounded-lg text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500 transition-all duration-200" %>
            </div>
            
            <!-- Active Filters Display -->
            <% if params[:search].present? || params[:event_type].present? || params[:date_range].present? || params[:action_filter].present? %>
              <div class="flex items-center space-x-2">
                <span class="text-sm text-gray-500">Active filters:</span>
                <% if params[:search].present? %>
                  <span class="inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-blue-100 text-blue-800">
                    Search: <%= params[:search] %>
                  </span>
                <% end %>
                <% if params[:event_type].present? %>
                  <span class="inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">
                    Type: <%= params[:event_type] %>
                  </span>
                <% end %>
                <% if params[:date_range].present? %>
                  <span class="inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-purple-100 text-purple-800">
                    Range: <%= params[:date_range] %>
                  </span>
                <% end %>
                <% if params[:action_filter].present? %>
                  <span class="inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-orange-100 text-orange-800">
                    Action: <%= params[:action_filter] %>
                  </span>
                <% end %>
              </div>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>

    <!-- Events List -->
    <div class="bg-white/80 backdrop-blur-sm shadow-xl rounded-2xl border border-gray-100">
      <div class="px-6 py-6 border-b border-gray-200/60">
        <div class="flex items-center justify-between">
          <div>
            <h3 class="text-lg font-semibold text-gray-900">
              Events (<%= @events.total_count || @events.count %>)
            </h3>
            <div class="text-sm text-gray-500">
              Real-time event monitoring
            </div>
          </div>
        </div>
      </div>
      <% if @events.present? %>
        <div class="divide-y divide-gray-200/60">
          <% @events.each do |event| %>
            <div class="px-6 py-5 hover:bg-blue-50/50 transition-all duration-200 group">
              <div class="flex items-center justify-between">
                <div class="flex items-center space-x-4">
                  <!-- Event Icon -->
                  <div class="flex-shrink-0">
                    <div class="h-12 w-12 bg-gradient-to-r from-blue-500 to-blue-600 rounded-xl flex items-center justify-center shadow-lg">
                      <svg class="h-6 w-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                      </svg>
                    </div>
                  </div>
                  
                  <!-- Event Details -->
                  <div class="flex-1 min-w-0">
                    <div class="flex items-center space-x-3 mb-2">
                      <%= link_to event.event_type, events_path(event_type: event.event_type), class: "text-sm font-semibold text-blue-700 hover:text-blue-800 hover:underline transition-colors duration-200" %>
                      <span class="inline-flex items-center px-2.5 py-1 rounded-full text-xs font-medium bg-blue-100 text-blue-800">
                        <%= event.action&.titleize || 'Unknown' %>
                      </span>
                      <% if event.correlation_id.present? && current_user.company.events.where(correlation_id: event.correlation_id).count > 1 %>
                        <%= link_to dashboard_story_path(event.correlation_id), class: "inline-flex items-center px-2.5 py-1 rounded-full text-xs font-medium bg-purple-100 text-purple-800 hover:bg-purple-200 transition-colors duration-200" do %>
                          <svg class="w-3 h-3 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>
                          </svg>
                          Story (<%= current_user.company.events.where(correlation_id: event.correlation_id).count %> events)
                        <% end %>
                      <% end %>
                    </div>
                    
                    <div class="text-sm text-gray-600 mb-1">
                      <% if event.actor.present? && event.actor['type'].present? && event.actor['id'].present? %>
                        <span class="font-medium text-gray-700"><%= event.actor['type'].humanize %></span>
                        <span class="text-gray-500">•</span>
                        <span class="font-mono text-gray-600"><%= event.actor['id'] %></span>
                      <% else %>
                        <span class="text-gray-500">Unknown Actor</span>
                      <% end %>
                      
                      <% if event.subject.present? && event.subject['type'].present? && event.subject['id'].present? %>
                        <span class="text-gray-500">•</span>
                        <span class="font-medium text-gray-700"><%= event.subject['type'].humanize %></span>
                        <span class="text-gray-500">•</span>
                        <span class="font-mono text-gray-600"><%= event.subject['id'] %></span>
                      <% end %>
                    </div>
                    
                    <div class="text-xs text-gray-400">
                      <%= event.timestamp.strftime("%b %d, %Y at %I:%M %p") %>
                      <span class="mx-2">•</span>
                      <%= time_ago_in_words(event.timestamp) %> ago
                    </div>
                  </div>
                </div>
                
                <!-- Action Buttons -->
                <div class="flex items-center space-x-2 opacity-0 group-hover:opacity-100 transition-opacity duration-200">
                  <%= link_to event_path(event), class: "inline-flex items-center px-3 py-2 border border-gray-300 shadow-sm text-sm font-medium rounded-lg text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500 transition-all duration-200" do %>
                    <svg class="w-4 h-4 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"></path>
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M2.458 12C3.732 7.943 7.523 5 12 5c4.478 0 8.268 2.943 9.542 7-1.274 4.057-5.064 7-9.542 7-4.477 0-8.268-2.943-9.542-7z"></path>
                    </svg>
                    View
                  <% end %>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      <% else %>
        <div class="px-6 py-16 text-center">
          <svg class="mx-auto h-16 w-16 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v10a2 2 0 002 2h8a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2"></path>
          </svg>
          <h3 class="mt-4 text-lg font-medium text-gray-900">No events found</h3>
          <p class="mt-2 text-sm text-gray-500">Try adjusting your filters or check back later for new events.</p>
          <div class="mt-6">
            <%= link_to events_path, class: "inline-flex items-center px-4 py-2 border border-transparent text-sm font-medium rounded-lg text-white bg-blue-600 hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500 transition-colors duration-200" do %>
              <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15"></path>
              </svg>
              Clear Filters
            <% end %>
          </div>
        </div>
      <% end %>
    </div>

    <!-- Enhanced Pagination -->
    <% if @events.respond_to?(:current_page) && @events.total_pages > 1 %>
      <div class="bg-white/80 backdrop-blur-sm shadow-xl rounded-2xl border border-gray-100 mt-8">
        <div class="px-6 py-4 flex items-center justify-between">
          <div class="flex-1 flex justify-between sm:hidden">
            <% if @events.previous_page %>
              <%= link_to events_path(page: @events.previous_page, **request.query_parameters), class: "relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-lg text-gray-700 bg-white hover:bg-gray-50 transition-all duration-200" do %>
                <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7"></path>
                </svg>
                Previous
              <% end %>
            <% end %>
            <% if @events.next_page %>
              <%= link_to events_path(page: @events.next_page, **request.query_parameters), class: "ml-3 relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-lg text-gray-700 bg-white hover:bg-gray-50 transition-all duration-200" do %>
                Next
                <svg class="w-4 h-4 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                </svg>
              <% end %>
            <% end %>
          </div>
          <div class="hidden sm:flex-1 sm:flex sm:items-center sm:justify-between">
            <div>
              <p class="text-sm text-gray-700">
                Showing <span class="font-medium"><%= (@events.current_page - 1) * @events.limit_value + 1 %></span> to 
                <span class="font-medium"><%= [@events.current_page * @events.limit_value, @events.total_count].min %></span> of 
                <span class="font-medium"><%= @events.total_count %></span> results
              </p>
            </div>
            <div>
              <nav class="relative z-0 inline-flex rounded-lg shadow-sm -space-x-px" aria-label="Pagination">
                <% if @events.previous_page %>
                  <%= link_to events_path(page: @events.previous_page, **request.query_parameters), class: "relative inline-flex items-center px-3 py-2 rounded-l-lg border border-gray-300 bg-white text-sm font-medium text-gray-500 hover:bg-gray-50 transition-all duration-200" do %>
                    <span class="sr-only">Previous</span>
                    <svg class="h-5 w-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7"></path>
                    </svg>
                  <% end %>
                <% end %>
                
                <% (@events.total_pages).times do |page_num| %>
                  <% page = page_num + 1 %>
                  <% if page == @events.current_page %>
                    <span class="relative inline-flex items-center px-4 py-2 border border-blue-500 bg-blue-50 text-sm font-medium text-blue-600 rounded-lg">
                      <%= page %>
                    </span>
                  <% elsif page <= @events.current_page + 2 && page >= @events.current_page - 2 %>
                    <%= link_to page, events_path(page: page, **request.query_parameters), class: "relative inline-flex items-center px-4 py-2 border border-gray-300 bg-white text-sm font-medium text-gray-700 hover:bg-gray-50 transition-all duration-200" %>
                  <% end %>
                <% end %>
                
                <% if @events.next_page %>
                  <%= link_to events_path(page: @events.next_page, **request.query_parameters), class: "relative inline-flex items-center px-3 py-2 rounded-r-lg border border-gray-300 bg-white text-sm font-medium text-gray-500 hover:bg-gray-50 transition-all duration-200" do %>
                    <span class="sr-only">Next</span>
                    <svg class="h-5 w-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                    </svg>
                  <% end %>
                <% end %>
              </nav>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

<% content_for :styles do %>
  <style>
    .severity-info { @apply bg-blue-100; }
    .severity-warning { @apply bg-yellow-100; }
    .severity-error { @apply bg-red-100; }
    .severity-critical { @apply bg-purple-100; }
    
    .badge-info { @apply bg-blue-100 text-blue-800; }
    .badge-warning { @apply bg-yellow-100 text-yellow-800; }
    .badge-error { @apply bg-red-100 text-red-800; }
    .badge-critical { @apply bg-purple-100 text-purple-800; }
  </style>
<% end %>

<% content_for :scripts do %>
  <script>
    function severityColor(severity) {
      const colors = {
        'info': 'bg-blue-100',
        'warning': 'bg-yellow-100', 
        'error': 'bg-red-100',
        'critical': 'bg-purple-100'
      };
      return colors[severity?.toLowerCase()] || 'bg-gray-100';
    }
    
    function severityBadgeColor(severity) {
      const colors = {
        'info': 'bg-blue-100 text-blue-800',
        'warning': 'bg-yellow-100 text-yellow-800',
        'error': 'bg-red-100 text-red-800', 
        'critical': 'bg-purple-100 text-purple-800'
      };
      return colors[severity?.toLowerCase()] || 'bg-gray-100 text-gray-800';
    }
  </script>
<% end %>
